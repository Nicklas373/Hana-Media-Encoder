<?xml version="1.0" encoding="utf-8"?>
<MetroSetTheme>
  <Theme>
    <Name>Hana Media Encoder MetroSetTheme</Name>
    <Author>Hana-CI Build Project</Author>
  </Theme>
  <Form>
	  <SmallLineColor1>#dbdbdb</SmallLineColor1>
    <SmallLineColor2>#dbdbdb</SmallLineColor2>
  </Form>
   <DefaultButton>
    <NormalColor>#161B21</NormalColor>
    <NormalBorderColor>#F4A950</NormalBorderColor>
    <NormalTextColor>#F4A950</NormalTextColor>
    <HoverColor>#F4A950</HoverColor>
    <HoverBorderColor>#F4A950</HoverBorderColor>
    <HoverTextColor>#161B21</HoverTextColor>
    <PressColor>#F4A950</PressColor>
    <PressBorderColor>#F4A950</PressBorderColor>
    <PressTextColor>#161B21</PressTextColor>
    <DisabledBackColor>#161B21</DisabledBackColor>
    <DisabledBorderColor>#161B21</DisabledBorderColor>
    <DisabledForeColor>#161B21</DisabledForeColor>
  </DefaultButton>
  <Divider>
    <Orientation>Horizontal</Orientation>
    <!--Horizontal, Vertical-->
    <Thickness>1</Thickness>
    <ForeColor>#F4A950</ForeColor>
  </Divider>
  <TabControl>
    <BackColor>#161B21</BackColor>
    <ForeColor>#F4A950</ForeColor>
	  <UnselectedTextColor>#F4A950</UnselectedTextColor>
    <SelectedTextColor>#dbdbdb</SelectedTextColor>
  </TabControl>
  <RadioButton>
	<ForeColor>#F4A950</ForeColor>
	<CheckColor>#F4A950</CheckColor>
	<BackColor>#161B21</BackColor>
	<BordeColor>#dbdbdb</BordeColor>
	<DisabledBorderColor>#dbdbdb</DisabledBorderColor>
  </RadioButton>
  <CheckBox>
	<ForeColor>#F4A950</ForeColor>
	<CheckColor>#F4A950</CheckColor>
	<BackColor>#161B21</BackColor>
	<BordeColor>#F4A950</BordeColor>
	<DisabledBorderColor>#dbdbdb</DisabledBorderColor>
	<CheckedStyle>Sign</CheckedStyle>
	<!--Sign, Shape-->
  </CheckBox>
  <ComboBox>
	<ForeColor>#F4A950</ForeColor>
	<BackColor>#161B21</BackColor>
	<BorderColor>#F4A950</BorderColor>
	<ArrowColor>#F4A950</ArrowColor>
	<SelectedItemBackColor>#F4A950</SelectedItemBackColor>
	<SelectedItemForeColor>#161B21</SelectedItemForeColor>
	<DisabledBackColor>#dbdbdb</DisabledBackColor>
	<DisabledBorderColor>#dbdbdb</DisabledBorderColor>
	<DisabledForeColor>#dbdbdb</DisabledForeColor>
  </ComboBox>
   <Label>
    <ForeColor>#F4A950</ForeColor>
    <BackColor>#161B21</BackColor>
  </Label>
</MetroSetTheme>
